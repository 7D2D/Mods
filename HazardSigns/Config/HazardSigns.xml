<configs>
	<!--  This tells SDX to add to the blocks.xml -->
	<config name="blocks">	
		<!-- This tells SDX to add the following block to the bottom of the block list -->
		<append xpath="/blocks">	
			<block id="" name="HazardSignBio"> <!-- All signs are free Unity store assets, by SB Laboratory -->
				<property name="CreativeMode" value="Dev"/>
				<property name="Shape" value="ModelEntity" />
				<property name="Model" value="#HazardSigns?HazardSignBioPrefab" /> 
				<property name="CustomIcon" value="ui_game_symbol_hazardSign" />
				<property name="MultiBlockDim" value="1,2,2" />
				<property name="Material" value="Mmetal_medium"/>
				<property name="IsTerrainDecoration" value="true"/>
				<property name="OnlySimpleRotations" value="true"/>
				<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
				<drop event="Destroy" name="scrapIron" count="5,15" prob="1"/>
